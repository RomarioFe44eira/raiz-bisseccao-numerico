#!/usr/bin/gnuplot
set terminal pngcairo transparent enhanced font "arial, 10" fontscale 1.0 size 600, 400;
set title "Método da Bissecção - Gráfico";
set output "charts.png";
set size 1,1;
set grid;
set yrange [-10:10];
set yrange [-10:10];
set ytics -10,1,10;
set xtics -10,1,10;
plot log(x) w l lt 1 lw 1 t "log(x)";
